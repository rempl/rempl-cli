{
  "name": "rempl-cli",
  "version": "1.2.1",
  "description": "Command line app to launch rempl server instance",
  "author": "Roman Dvornov",
  "license": "MIT",
  "repository": "rempl/rempl-cli",
  "keywords": [
    "rempl",
    "server",
    "cli"
  ],
  "type": "module",
  "bin": {
    "rempl": "./bin/rempl"
  },
  "scripts": {
    "lint": "eslint scripts/*.cjs src && npm run ts-check",
    "ts-check": "tsc --noEmit",
    "watch": "node scripts/transpile.cjs --watch",
    "start": "node ./bin/rempl --"
  },
  "dependencies": {
    "clap": "~3.1.1",
    "express": "^4.18.2",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "esbuild": "^0.19.6",
    "eslint": "^8.54.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.8.8",
    "rempl": "^1.0.0-alpha.21",
    "rollup": "^4.5.0",
    "socket.io-client": "^4.7.2",
    "sucrase": "^3.34.0",
    "typescript": "^5.2.2"
  }
}
