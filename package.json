{
  "name": "rempl-cli",
  "version": "1.2.1",
  "description": "Command line app to launch rempl server instance",
  "author": "Roman Dvornov",
  "license": "MIT",
  "repository": "rempl/rempl-cli",
  "keywords": [
    "rempl",
    "server",
    "cli"
  ],
  "type": "module",
  "bin": {
    "rempl": "./bin/rempl"
  },
  "scripts": {
    "lint": "eslint scripts/*.cjs src && npm run ts-check",
    "ts-check": "tsc --noEmit",
    "build": "node scripts/transpile.cjs",
    "watch": "node scripts/transpile.cjs --watch",
    "start": "node ./bin/rempl"
  },
  "dependencies": {
    "clap": "~3.1.1",
    "express": "^4.18.2",
    "pem": "^1.14.8",
    "socket.io": "^2.5.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.2",
    "@types/pem": "^1.14.4",
    "@types/socket.io": "^2.1.13",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "esbuild": "^0.19.6",
    "eslint": "^8.54.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "rollup": "^4.5.0",
    "sucrase": "^3.34.0",
    "typescript": "~5.2.2"
  }
}
